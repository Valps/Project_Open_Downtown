 IF ( flag_all_missions_passed = 21 )          // Check if player has completed all gang missions
	   IF ( flag_on_mission = 0 )                 // Insure that the player isn't playing a mission
	      LAUNCH_MISSION (wil_end.mis)            // Launch End Mission
	   ENDIF
 ENDIF
 
RETURN



