//XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
//XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX MAIN LOOP XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
//XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

//   Version 1.1.0: the main_loop subroutine has been reworked with the help of T.M. decompiler in debug mode. Now this subroutine is EXACTLY the same of the original wil.scr

main_loop:
  
  WHILE_EXEC ( true = 1 )     // Infinite Loop
  
     WHILE_EXEC ( flag_on_mission = 1 )     //  If the player is playing a mission, do nothing
        DO_NOWT
        ENDEXEC
     ENDWHILE
     
     //  Now the player isn't playing a mission
     
     WHILE_EXEC ( NOT ( CHECK_SCORE_GREATER(p1, 999999) ) )   //  If the player doesnt have the final score
        
        DO_NOWT
        
        WHILE_EXEC ( flag_on_mission = 1 )      //  If the player has entered a mission before getting the final score, do nothing
           DO_NOWT
           ENDEXEC
        ENDWHILE
        
        ENDEXEC
        
     ENDWHILE
     
     //  The player got the final score and he isn't playing a mission
     
     CLEAR_ALL_BRIEFS ()
     DISPLAY_BRIEF_NOW (1989)                                         //  "You did it! Follow the big pink marker to leave the Downtown District!"
     LEVEL_END_POINT_ARROW_AT (arrow_end, 153.50, 138.50, 2.00)       //  Point at the church
     
     WHILE_EXEC ( ( flag_on_mission = 0 ) AND ( CHECK_SCORE_GREATER(p1, 999999) ) )     //  While the player isn't playing a mission and has the final score
        
        IF ( LOCATE_CHARACTER_ANY_MEANS(p1, 153.50, 138.50, 2.00, 1.00, 1.00) )         //  Waits the player to arrive at the church to finish Downtown District
           REMOVE_ARROW (arrow_end)              // Remove blinking arrow
           FINISH_LEVEL (BONUS_1)                // Finish the Downtown District
        ENDIF
        
        ENDEXEC
        
     ENDWHILE
     
     //  Now either the player doesn't have 1 million of score or he has entered a mission
     
     REMOVE_ARROW (arrow_end)     //  Therefore remove the end arrow
  
  ENDWHILE                 // End Infinite Loop
  
  
RETURN


